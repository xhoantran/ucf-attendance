    "happy-dom": "^12.1.6",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "postcss": "8.4.21",
    "prettier": "2.8.2",
    "tailwindcss": "3.2.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.8",
    "vite-preset-react": "2.3.0",
    "vitest": "^0.34.5"
  }
